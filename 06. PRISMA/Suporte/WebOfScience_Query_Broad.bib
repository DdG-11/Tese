@article{ WOS:001496418000001,
Author = {Zhao, Wenbing and Akella, Vyaghri Seetharamayya and Yang, Shunkun and
   Luo, Xiong},
Title = {Machine Learning in Baseball Analytics: Sabermetrics and Beyond},
Journal = {INFORMATION},
Year = {2025},
Volume = {16},
Number = {5},
Month = {APR 29},
Abstract = {In this article, we provide a comprehensive review of machine
   learning-based sports analytics in baseball. This review is primarily
   guided by the following three research questions: (1) What baseball
   analytics problems have been studied using machine learning? (2) What
   data repositories have been used? (3) What and how machine learning
   techniques have been employed for these studies? The findings of these
   research questions lead to several research contributions. First, we
   provide a taxonomy for baseball analytics problems. According to the
   proposed taxonomy, machine learning has been employed to (1) predict
   individual game plays; (2) determine player performance; (3) estimate
   player valuation; (4) predict future player injuries; and (5) project
   future game outcomes. Second, we identify a set of data repositories for
   baseball analytics studies. The most popular data repositories are
   Baseball Savant and Baseball Reference. Third, we conduct an in-depth
   analysis of the machine learning models applied in baseball analytics.
   The most popular machine learning models are random forest and support
   vector machine. Furthermore, only a small fraction of studies have
   rigorously followed the best practices in data preprocessing, machine
   learning model training, testing, and prediction outcome interpretation.},
DOI = {10.3390/info16050361},
Article-Number = {361},
EISSN = {2078-2489},
ResearcherID-Numbers = {Zhao, Wenbing/H-5498-2019
   Luo, Xiong/P-4343-2016
